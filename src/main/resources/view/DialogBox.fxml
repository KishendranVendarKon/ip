<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<fx:root maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
  <children>
    <ImageView fx:id="displayPicture" fitHeight="100.0" fitWidth="120.0" pickOnBounds="true" preserveRatio="true" />
      <ScrollPane hbarPolicy="NEVER" prefHeight="100.0" vmax="1.0" vvalue="0.0">
        <content>
            <Label fx:id="dialog" contentDisplay="CENTER" maxHeight="-Infinity" maxWidth="550.0" minHeight="98.0" style="-fx-background-color: WHITE;" text="Label" wrapText="true">
               <font>
                  <Font name="Arial" size="14.0" />
               </font>
               <padding>
                  <Insets left="10.0" right="20.0" top="4.0" />
               </padding></Label>
        </content>
      </ScrollPane>
  </children>
  <padding>
    <Insets bottom="15.0" left="5.0" right="5.0" top="15.0" />
  </padding>
</fx:root>
